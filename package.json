{
  "name": "vue-stucture",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint --ext js,ts,tsx src",
    "format": "prettier --write",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "*.{ts, vue}": [
      "pnpm run lint",
      "pnpm run format",
      "pnpm run build"
    ]
  }
}
